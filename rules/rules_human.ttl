@prefix PR: <http://example.org/> .
@prefix VAR: <http://variable.org/> .

PR:rule_human_socrates
    PR:operation PR:human ;
    PR:variable_x PR:socrates .
PR:rule_human_mortal
    PR:left_side [
        PR:operation PR:mortal ;
        PR:variable_x VAR:variable_x ] ;
    PR:right_side [
        PR:priority "1" ;
        PR:child [
            PR:operation PR:human ;
            PR:variable_x VAR:variable_x ] ] .
