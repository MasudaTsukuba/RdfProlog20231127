    PR:variable_x PR:socrates .

# rules
PR:rule_human_mortal
    PR:left_side [
        PR:operation PR:mortal ;
        PR:variable_x VAR:x ] ;
    PR:right_side [
        PR:priority "1" ;
        PR:child [
            PR:operation PR:human ;
            PR:variable_x VAR:x ] ] .

