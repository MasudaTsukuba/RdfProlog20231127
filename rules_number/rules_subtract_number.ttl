##################################################################
# rules
PR:rule_subtract_number_x_y_z
    PR:type PR:rule ;
    PR:left_side [
        PR:operation PR:subtract_number ;
        PR:variable_x VAR:_x ;
        PR:variable_y VAR:_y ;
        PR:variable_z VAR:_z ] ;
    PR:right_side [
        PR:child [
            PR:operation PR:add_number ;
            PR:variable_x VAR:_y ;
            PR:variable_y VAR:_z ;
            PR:variable_z VAR:_x ] ].

##################################################################
# controls
PR:control_subtract_number_x_y_z
    PR:type PR:control ;
    PR:left_side [
        PR:operation PR:subtract_number ;
        PR:variable_x VAR:_x ;
        PR:variable_y VAR:_y ;
        PR:variable_z VAR:_z ] ;
    PR:right_side [
        PR:child [
            PR:operation PR:add_number ;
            PR:variable_x VAR:_y ;
            PR:variable_y VAR:_z ;
            PR:variable_z VAR:_x ] ].

##################################################################
# applications
PR:application_subtract_number_x_y_z
    PR:type PR:application ;
    PR:program PR:false ;
    PR:pattern [
        PR:operation PR:subtract_number ;
        PR:variable_x VAR:_x ;
        PR:variable_y VAR:_y ;
        PR:variable_z VAR:_z ] ;
    PR:use [
        PR:control PR:control_subtract_number_x_y_z ] .
